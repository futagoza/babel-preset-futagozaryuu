    "babel-plugin-transform-async-to-module-method": "6.24.1"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-futagozaryuu": "2.2.0",
    "babel-cli": "6.24.1"
  },
  "engines": {
    "node": ">= 4"
  },
  "eslintConfig": {
    "extends": "futagozaryuu/node-v4",
    "root": true
  }
