{
    "name": "babel-preset-futagozaryuu",
    "version": "2.12.0",
    "description": "Futago-za Ryuu's Babel preset",
    "keywords": [
        "babel",
        "preset",
        "plugin",
        "ecmascript",
        "javascript"
    ],
    "repository": "futagoza/babel-preset-futagozaryuu",
    "bugs": "https://github.com/futagoza/babel-preset-futagozaryuu/issues",
    "license": "MIT",
    "author": "Futago-za Ryuu",
    "files": [
        "lib",
        "scripts"
    ],
    "main": "lib/preset.js",
    "scripts": {
        "lint": "eslint --cache . src/.*rc.js",
        "build": "babel src --out-dir lib",
        "preview-configurations": "node preview/generate-configurations --runtime",
        "preview-result": "node preview/generate-result --runtime",
        "test": "eslint --quiet src && npm run build && npm run preview-result",
        "postversion": "git push --follow-tags && npm publish"
    },
    "dependencies": {
        "@babel/polyfill": "7.0.0-beta.34",
        "@babel/runtime": "7.0.0-beta.34",
        "@babel/core": "7.0.0-beta.34",
        "@babel/preset-stage-0": "7.0.0-beta.34",
        "@babel/preset-env": "7.0.0-beta.34",
        "@babel/plugin-transform-runtime": "7.0.0-beta.34",
        "babel-plugin-module-resolver": "3.0.0"
    },
    "devDependencies": {
        "eslint": ">= 4.12.0",
        "eslint-config-futagozaryuu": "3.7.x",
        "minimist": "1.2.0",
        "mkdirp": "0.5.1",
        "@babel/cli": "7.0.0-beta.34"
    },
    "engines": {
        "node": ">= 6"
    },
    "eslintConfig": {
        "extends": "futagozaryuu/node-v6",
        "root": true
    }
}
