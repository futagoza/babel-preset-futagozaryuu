{
  "name": "babel-preset-futagozaryuu",
  "version": "2.0.0",
  "description": "Futago-za Ryuu's Babel preset",
  "keywords": [
    "babel",
    "preset",
    "plugin",
    "ecmascript",
    "javascript"
  ],
  "repository": "futagoza/babel-preset-futagozaryuu",
  "bugs": "https://github.com/futagoza/babel-preset-futagozaryuu/issues",
  "license": "MIT",
  "author": "Futago-za Ryuu",
  "files": [
    "lib"
  ],
  "main": "lib/preset.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "preversion": "npm install && npm test",
    "postversion": "git push --follow-tags && npm publish",
    "lint": "eslint --cache src preview.js",
    "preset": "babel --no-babelrc src --out-dir lib/test --presets ./lib/preset.js",
    "test": "eslint --quiet src && npm run build && npm run preset"
  },
  "dependencies": {
    "babel-core": "7.0.0-alpha.9",
    "babel-preset-stage-0": "7.0.0-alpha.9",
    "babel-preset-env": "2.0.0-alpha.7",
    "babel-plugin-module-resolver": "3.0.0-beta.0",
    "babel-plugin-transform-async-to-module-method": "7.0.0-alpha.9"
  },
  "devDependencies": {
    "eslint": "3.19.0",
    "eslint-config-futagozaryuu": "2.2.0",
    "minimist": "1.2.0",
    "babel-cli": "7.0.0-alpha.9"
  },
  "engines": {
    "node": ">= 4"
  },
  "eslintConfig": {
    "extends": "futagozaryuu/node-v4",
    "root": true
  }
}
